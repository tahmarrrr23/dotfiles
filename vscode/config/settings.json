{
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "PlemolJP, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 16,
  "editor.guides.bracketPairs": true,
  "editor.renderWhitespace": "boundary",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/__pycache__": true
  },
  "files.insertFinalNewline": true,
  "git.confirmSync": false,
  "path-intellisense.absolutePathToWorkspace": false,
  "shellcheck.exclude": [
    "1091"
  ],
  "shellcheck.executablePath": "/opt/homebrew/bin/shellcheck",
  "sortJSON.contextMenu": {
    "sortJSON": true,
    "sortJSONAlphaNum": false,
    "sortJSONAlphaNumReverse": false,
    "sortJSONKeyLength": false,
    "sortJSONKeyLengthReverse": false,
    "sortJSONReverse": false,
    "sortJSONType": false,
    "sortJSONTypeReverse": false,
    "sortJSONValues": false,
    "sortJSONValuesReverse": false
  },
  "window.zoomLevel": 1,
  "workbench.colorTheme": "SynthWave '84",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.tree.indent": 10,
  "workbench.tree.renderIndentGuides": "always"
}
